def get_num_size(num : int) : int
    size = 0 :
    (while num != 0
    do num = num / 10 : size = size + 1) :
    size ;

def modulo(num1 : int, num2 : int) : int
    tmp1 = num1 / num2 :
    tmp2 = tmp1 * num2 :
    tmp3 = num1 - tmp2 :
    tmp3 ;

def print_num(num : int, size : int) : int
    tmp = num / (10 * size) :
    (while tmp != 0
    do print(tmp % 10) : size = size - 1 : tmp = num / (10 * size)) :
    size ;

print_num(12);